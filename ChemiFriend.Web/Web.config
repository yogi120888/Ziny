    <add name="log4netconnectionstring" connectionString="server=YOGENDRA-PC\SQLEXPRESS;database=DB_ChemiFriend;uid=sa;pwd=root;persist security info=true;multipleactiveresultsets=true;" providerName="system.data.sqlclient" />
  
